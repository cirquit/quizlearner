/* Background */

body {
 background-color: #CCC;
 font-family: Helvetica, Arial;
}

.front {
  background-color: #2B2B2B;
}

/* Font Styles */
.mainTitle, .mainTitle:hover {
  font-size: 64px;
  font-weight: 900;
  color: #EEE;
  display: inline-block;
  margin-top: 20px;
  margin-left: 60px;
  text-decoration: none;
}

.green {
  font-size: 24px;
  text-align: middle;
  color:#82CC68;
  font-weight: bold;
  margin-left: 10px;
}

.sadred {
  font-size: 24px;
  text-align: middle;
  color:#E52325;
  font-weight: bold;
  margin-left: 10px;
}

.orangeTitle{
  font-size: 26px;
  font-weight: 900;
  color:#FFA500;
  margin-top: 8px;
}

.boldWhite {
  font-size: 24px;
  text-align: left;
  color:#CCC;
  font-weight: bold;
  margin-left: 10px;
}

.simpleWhite {
  color: white;
  font-style: 20px;
  margin:10px;
}

.simpleOrange {
   color:#FFA500;
   font-style: 20px;
}


/* Left Exam List */

.examList{
  font-size: 20px;
  font-weight: 600;
  list-style-type: none;
}

.examList ul{
  text-align: center;
}

.examList li a{
  color: #EEE;
  display: inline;
}

.examList li a:hover{
  color: #FFA500;
  text-decoration: none;
}


/* Evaluation Table */

.evalTable{
    display: table;
    border-collapse: collapse;
    border-spacing: 10px;
    tr{
        th{
           padding: 5px;
           text-align: center;
           color:white;
           font-size: 20px;
           border: 1px solid white;
        }
  }
}

