/*Inner question div*/

#wrapper {
  width: 100%;
  height: 100%;
  min-height: 600px;
  min-width: 650px;
  margin-top: 25px;
  overflow: hidden;
}

#top {
  height: 142px;
  width: 100%;
  border-bottom: solid 4px #ccc;
  clear: both;
}

#left {
  float: left;
  height: auto;
  width: 25%;
  padding-bottom: 6000px;
  margin-bottom: -6000px;
  border-right:solid 2px #ccc;
}

#question {
  float: right;
  width: 75%;
  min-height: 600px;
  border-left: solid 2px #ccc;
}

.button {
  float: right;
  color: #CCC;
  font-size: 20px;
  background-color: #463E3F;
}


.tabs input[type=radio] {
          position: absolute;
          top: -9999px;
          left: -9999px;
      }
      .tabs {
        width: 700px;
        float: none;
        list-style: none;
        position: relative;
        padding: 0;
      }
      .tabs li{
        float: left;
        height: 40px;
        top: -10px;
      }
      .tabs label {
          display: block;
          color: #EEE;
          text-align: center;
          line-height: 40px;
          font-size: 18px;
          font-weight: normal;
          background: #333;
          cursor: pointer;
          position: relative;
          top:-10px;
          left:0px;
          width: 70px;
          height: 40px;
          -webkit-transition: all 0.2s ease-in-out;
          -moz-transition: all 0.2s ease-in-out;
          -o-transition: all 0.2s ease-in-out;
          transition: all 0.2s ease-in-out;
      }
      .tabs label:hover {
        background: rgba(255,255,255,0.5);
      }

      [id^=tab]:checked + label {
        background: #555;
        color: #FFA500;
      }

      [id^=tab]:checked ~ [id^=tab-content] {
          display: block;
      }
      .tab-content{
        z-index: 2;
        display: none;
        text-align: left;
        width: 772px;
        font-size: 20px;
        line-height: 140%;
        padding-top: 10px;
        background: #555;
        padding: 15px;
        position: absolute;
        width: 100%;
        left:0px;
        box-sizing: border-box;
        -webkit-animation-duration: 0.5s;
        -o-animation-duration: 0.5s;
        -moz-animation-duration: 0.5s;
        animation-duration: 0.5s;
      }


